<p id="notice"><%= notice %></p>

<h1>Tourneys</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Spreadsheet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tourneys.each do |tourney| %>
      <tr>
        <td><%= tourney.title %></td>
        <td><%= tourney.spreadsheet %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
